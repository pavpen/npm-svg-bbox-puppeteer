{
  "name": "@pavpen.dev/svg-bbox-puppeteer",
  "version": "0.0.0",
  "description": "Uses Puppeteer to render an SVG in a headless browser, and calculate its bounding box",
  "keywords": [
    "SVG",
    "bounding",
    "box"
  ],
  "license": "BSD-3-Clause",
  "author": "Pavel Penev <pavpen@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavpen/npm-svg-bbox-puppeteer"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test:units": "node --import=@swc-node/register/esm-register --enable-source-maps --test tests/**/test-*.ts",
    "test:integration": "node --import=@swc-node/register/esm-register --enable-source-maps scripts/run-integration-tests.ts",
    "build": "tsc"
  },
  "devDependencies": {
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.13.5",
    "@types/node": "^24.3.1",
    "commander": "^14.0.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "puppeteer-core": "^24.19.0"
  }
}